    "apollo-server-express": "^1.1.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint": "^4.8.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "express": "^4.16.2",
    "flow-bin": "^0.56.0",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.4.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.31",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "request-promise": "^4.2.2",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "build": "rm -rf build && babel ./ -d build --ignore node_modules --source-maps",
    "watch": "nodemon index.js --exec babel-node --presets env",
    "start": "npm run build && node ./build/index.js",
    "seed:up": "babel-node ./seeders/seeder.js up --presets env",
    "seed:down": "babel-node ./seeders/seeder.js down --presets env",
    "seed:reset": "babel-node ./seeders/seeder.js reset --presets env"
  }
